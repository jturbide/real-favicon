    "vite": "^7.3.1"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jturbide/real-favicon.git"
  },
  "keywords": [
    "favicon",
    "build-time",
    "vite",
    "ssg",
