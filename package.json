{
  "name": "@jturbide/real-favicon",
  "version": "0.1.0",
  "description": "Build-time favicon generation with a framework-agnostic core and optional Vite plugin.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup"
  },
  "dependencies": {
    "@realfavicongenerator/generate-favicon": "^0.3.0",
